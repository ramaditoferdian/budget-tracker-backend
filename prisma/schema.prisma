generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl  = env("DIRECT_URL")
}

model Transaction {
  id        String   @id @default(uuid())
  title     String
  amount    Float
  type      String   // 'income' atau 'expense'
  category  String
  createdAt DateTime @default(now())
}
